{
  "manifest_version": 2,
  "name": "Fohdeesha Desktop Sharing",
  "description": "A simple extension to allow desktop sharing within FBOM/Fohdeesha Jitsi.",
  "version": "1.2",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "logo-16x16.png",
    "48": "logo-48x48.png",
    "128": "logo-128x128.png"
  },
  "background": {
        "scripts": [ "background.js" ],
        "persistent": true
  },
  "permissions": [
      "desktopCapture"
  ],
  "externally_connectable": {
      "matches": [
          "*://meet.fohdeesha.com/*"
      ]
  }
}
